		pinctrl-0 = <0x5d 0x5e>;
		pinctrl-names = "default";
		ref_voltage = <0xcb2>;
		status = "okay";
	};

	adc-joystick {
		adc-drift = <0x0a>;
		compatible = "rockchip,joystick";
		io-channel-names = "adcX\0adcY";
		io-channels = <0x5b 0x00 0x5b 0x01>;
		status = "okay";

		akey {
			debounce-interval = <0x14>;
			gpios = <0x39 0x0b 0x01>;
			label = "a key";
			linux,code = <0x1c>;
		};

		bkey {
			debounce-interval = <0x14>;
			gpios = <0x39 0x09 0x01>;
			label = "b key";
			linux,code = <0x0e>;
		};

		downkey {
			adc,channel = <0x01>;
			gpios = <0x5c 0x17 0x01>;
			label = "down key";
			linux,code = <0x6c>;
		};

		l_key {
			debounce-interval = <0x14>;
			gpios = <0x39 0x08 0x01>;
			label = "left key";
			linux,code = <0x68>;
		};

		leftkey {
			adc,channel = <0x00>;
			gpios = <0x5c 0x18 0x01>;
			label = "left key";
			linux,code = <0x69>;
		};

		r_key {
			debounce-interval = <0x14>;
			gpios = <0x34 0x09 0x01>;
			label = "right key";
			linux,code = <0x6d>;
		};

		rightkey {
			adc,channel = <0x00>;
			gpios = <0x5c 0x19 0x01>;
			label = "right key";
			linux,code = <0x6a>;
		};

		upkey {
			adc,channel = <0x01>;
			gpios = <0x5c 0x16 0x01>;
			label = "up key";
			linux,code = <0x67>;
		};
	};

	adc-keys {
		compatible = "rockchip,key";
		io-channels = <0x5b 0x00>;
		status = "disabled";

		back-key {
			label = "back";
			linux,code = <0x25>;
			rockchip,adc_value = <0x2bc>;
		};

		vol-down-key {
			label = "volume down";
			linux,code = <0x4a>;
