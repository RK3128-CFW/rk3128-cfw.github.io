	adc-battery {
		auto_calibration = <0x00>;
		bat_table = <0x00 0x00 0x00 0x00 0xc8 0xc8 0xdb6 0xdc9 0xdcf 0xe28 0xe64 0xe83 0xec1 0xefa 0xf2e 0xf3d 0xf6f 0xdb6 0xdc9 0xdcf 0xe28 0xe64 0xe83 0xec1 0xefa 0xf2e 0xf3d 0xf6f>;
